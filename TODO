sample reblog yourself code:
$.get("http://cutlerish.tumblr.com/post/8761693408/my-internet-is-sucky", function(data) {
var url = data.match(/<\s*iframe[^>]*>/);
var rk;
for (i=0; url && i<url.length; i++) {
rk = url[i].match(/rk=([^&"']*)/);
if (rk && rk.length > 1) {
rk = rk[1];
}
else {
rk = null;
}
}
console.log(rk);
});

